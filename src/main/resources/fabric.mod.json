{
  "schemaVersion": 1,
  "id": "satchels",
  "version": "${version}",
  "name": "Satchels",
  "description": "Simple vanilla friendly mod that adds Satchels that can hold up to 3 items total.",
  "authors": ["Rosenoire", "Elysieon"],
  "contact": {},
  "license": "All-Rights-Reserved",
  "icon": "assets/satchels/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "net.rose.satchels.data.SatchelsDataGenerator"
    ],
    "client": [
      "net.rose.satchels.client.SatchelsClient"
    ],
    "main": [
      "net.rose.satchels.common.Satchels"
    ]
  },
  "mixins": [
    "satchels.mixins.json"
